}
.goodProduct .productTitle{
    width: 52%;
    height:0.34rem;
    margin-bottom: 0.38rem;
}
.goodProduct .product_1{
    width:100%;
    height: 2.62rem;
    line-height: 2.62rem;
    border-bottom: 1px solid #e6e6e6;
}
.goodProduct .product_2{
    width: 100%;
    height: 2.7rem;
    line-height: 2.7rem;
}
.travel{
    width: 100%;
    height: 2.8rem;
    padding: 0.4rem 0.2rem 0.4rem 0.2rem;
}
.travel .travelTtitle{
    width:83%;
